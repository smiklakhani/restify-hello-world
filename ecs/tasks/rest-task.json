{
    "containerDefinitions": [
    {
        "name": "rest", 
        "image": "nexus.hcqis.org:28443/restify-hello-world:v_%BUILD_NUMBER%", 
        "cpu": 10, 
        "memory": 256, 
        "portMappings": [
        {
            "protocol": "tcp",
            "containerPort": 8080,
            "hostPort": 0
        }
        ],
        "essential": true, 
        "dockerLabels": {
          "service": "rest"
        }
    }
  ],
  "family": "rest"
}
